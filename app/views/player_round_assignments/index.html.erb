<p id="notice"><%= notice %></p>

<h1>Player Round Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Bet league</th>
      <th>Player</th>
      <th>Round</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @player_round_assignments.each do |player_round_assignment| %>
      <tr>
        <td><%= player_round_assignment.bet_league_id %></td>
        <td><%= player_round_assignment.player_id %></td>
        <td><%= player_round_assignment.round_id %></td>
        <td><%= link_to 'Show', player_round_assignment %></td>
        <td><%= link_to 'Edit', edit_player_round_assignment_path(player_round_assignment) %></td>
        <td><%= link_to 'Destroy', player_round_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player Round Assignment', new_player_round_assignment_path %>
